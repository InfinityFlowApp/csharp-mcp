{
  "mcpServers": {
    "csharp-mcp": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--pull=always",
        "-v",
        "${HOME}:${HOME}",
        "-w",
        "${PWD}",
        "ghcr.io/infinityflowapp/csharp-mcp:latest"
      ],
      "env": {}
    }
  }
}
